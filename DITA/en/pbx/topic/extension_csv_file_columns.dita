            <entry>-</entry>
          </row>
          <row>
            <entry>transport</entry>
            <entry>
              <ul id="ul_nwk_bvx_scb">
                <li><b>tcp</b></li>
                <li><b>udp</b></li>
                <li><b>tls</b></li>
              </ul>
            </entry>
            <entry>Transport</entry>
            <entry>-</entry>
          </row>
          <row>
            <entry>enablesrtp</entry>
            <entry/>
            <entry>Enable SRTP</entry>
            <entry>-</entry>
          </row>
          <row>
            <entry>qualify</entry>
