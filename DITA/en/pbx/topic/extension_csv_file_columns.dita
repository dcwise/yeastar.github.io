            <entry>busyforward</entry>
            <entry>
              <ul>
                <li><b>yes</b></li>
                <li><b>no</b></li>
              </ul>
            </entry>
            <entry>When Busy</entry>
            <entry>-</entry>
          </row>
          <row>
            <entry>btransferto</entry>
            <entry>
              <ul>
                <li><b>voicemail</b></li>
                <li><b>extension</b></li>
                <li><b>usersmobile</b></li>
                <li><b>customnuber</b></li>
              </ul>
            </entry>
            <entry>-</entry>
            <entry>-</entry>
