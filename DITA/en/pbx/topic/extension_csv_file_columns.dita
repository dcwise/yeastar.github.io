          <row>
            <entry>loginpassword</entry>
            <entry/>
            <entry>User Password</entry>
            <entry>
              <ul>
                <li>The password must contain 3 types of characters: lowercase letters, uppercase
                  letters, and numbers.</li>
                <li>At least 6 characters.</li>
              </ul>
            </entry>
          </row>
          <row>
            <entry>dtmfmode</entry>
            <entry>
              <ul id="ul_nj2_cvx_scb">
                <li><b>rfc4733</b></li>
                <li><b>inband</b></li>
                <li><b>info</b></li>
                <li><b>auto</b></li>
              </ul>
            </entry>
            <entry>DTMF Mode</entry>
            <entry>-</entry>
          </row>
          <row>
            <entry>enableiprestrict</entry>
            <entry>
              <ul>
                <li><b>yes</b></li>
                <li><b>no</b></li>
              </ul>
            </entry>
            <entry>Enable IP Restriction</entry>
            <entry>-</entry>
          </row>
          <row>
            <entry>permitip</entry>
