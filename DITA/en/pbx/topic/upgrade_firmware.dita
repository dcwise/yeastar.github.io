<topic id="topic_kfc_wsl_ldb">
    <title>Upgrade Firmware</title>
    <shortdesc><ph product="s-series">You have multiple ways to update the PBX firmware.
        </ph></shortdesc>
    <prolog>
        <critdates>
            <!--Carol-->
            <revised modified="2018/06/14"/>
