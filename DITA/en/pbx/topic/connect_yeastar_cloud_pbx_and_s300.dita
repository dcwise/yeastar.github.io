<topic id="task_k5j_qqn_42b">
    <title>Step1. Connect Yeastar Cloud PBX and S300</title>
    <shortdesc></shortdesc>
    <prolog>
        <author type="creator">Carol</author>
        <critdates>
            <created date="2018/07/23"/>
            <!--Carol-->
            <revised modified="2018/07/23"/>
