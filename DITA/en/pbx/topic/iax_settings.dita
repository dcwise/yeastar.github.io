        </critdates>
    </prolog>
    <conbody>
        <section id="section_lmx_2rb_ffb">
            <title>IAX General Settings</title>
            <p>
                <table frame="all" rowsep="1" colsep="1" id="table_zxp_3rb_ffb">
                    <tgroup cols="2">
                        <colspec colname="c1" colnum="1" colwidth="1*"/>
